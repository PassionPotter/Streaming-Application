          <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="BeanShell PreProcessor" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import java.util.Arrays;
import java.util.HashSet;
import java.util.Set;
import java.lang.Boolean;

String s = &quot;${isAdult}&quot;.toLowerCase();
Set trueSet = new HashSet();
trueSet.add(&quot;1&quot;);
trueSet.add(&quot;true&quot;);
trueSet.add(&quot;yes&quot;);

Set falseSet = new HashSet();
falseSet.add(&quot;0&quot;);
falseSet.add(&quot;false&quot;);
falseSet.add(&quot;no&quot;);

Boolean trueValue=trueSet.contains(s);
Boolean falseValue=trueSet.contains(s);
Boolean finalValue=trueValue?trueValue:falseValue;


vars.put(&quot;isAdultFormattedValue&quot;,finalValue+&quot;&quot;);

log.info(&quot;Formatted Value\t&quot;+vars.get(&quot;isAdultFormattedValue&quot;));


</stringProp>
          </BeanShellPreProcessor>
